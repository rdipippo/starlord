{
  "name": "phaser3-advanced-boilerplate",
  "version": "1.0.0",
  "description": "Phaser 3 advanced boilerplate",
  "main": "index.js",
  "repository": "git@github.com:RiCoTeRoX/phaser3-advanced-boilerplate.git",
  "author": "danny <ricoterox@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "clean:dist": "rimraf ./dist",
    "message": "babel-node ./build/message.js",
    "build:dev": "set NODE_ENV=local&& babel-node build/build-dev.js",
    "build:prod": "babel-node ./build/build-prod.js",
    "predev": "set NODE_ENV=local&& npm-run-all --parallel clean:dist message",
    "dev": "set NODE_ENV=local&& npm-run-all build:dev",
    "preprod": "set NODE_ENV=production&& npm-run-all clean:dist message",
    "prod": "set NODE_ENV=production&& npm-run-all --parallel build:prod",
    "start": "npm-run-all dev"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "colors": "^1.2.5",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "^2.4.0",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "npm-run-all": "^4.1.3",
    "path": "^0.12.7",
    "prettier": "^1.12.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "string-replace-webpack-plugin": "^0.1.3",
    "url-loader": "^0.6.2",
    "webpack": "^3.12.0",
    "webpack-config": "^7.5.0",
    "webpack-dev-server": "^2.10.0",
    "webpack-hot-middleware": "^2.22.2"
  },
  "dependencies": {
    "phaser": "^3.3.0"
  }
}
